<?xml version="1.0" encoding="UTF-8" ?>

<phpdoc>
    <parser>
        <target>docs</target>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <files>
        <directory>./src</directory>
        <ignore>example/*</ignore>
        <ignore>service/*</ignore>
        <ignore>XChangeV1.php</ignore>
    </files>
    <transformations>
        <template name="clean" />
        <template name="checkstyle" />
    </transformations>
</phpdoc>